<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lxkj.mapper.MemberAddressMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="com.lxkj.entity.MemberAddress">
                    <id column="id" property="id" />
                    <result column="member_id" property="memberId" />
                    <result column="recipient" property="recipient" />
                    <result column="mobile" property="mobile" />
                    <result column="province" property="province" />
                    <result column="city" property="city" />
                    <result column="county" property="county" />
                    <result column="address" property="address" />
                    <result column="default" property="default" />
        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="Base_Column_List">
        id, member_id, recipient, mobile, province, city, county, address, default
    </sql>

</mapper>
